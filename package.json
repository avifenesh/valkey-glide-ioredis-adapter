{
  "name": "valkey-glide-ioredis-adapter",
  "version": "0.5.0",
  "description": "Drop-in ioredis replacement powered by Valkey GLIDE's high-performance Rust core. Complete Valkey JSON module support. Zero code changes required for most production use cases.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "COMPATIBILITY.md",
    "MIGRATION.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "pretest": "npm run build",
    "test": "./scripts/test.sh",
    "test:quick": "./scripts/test.sh --quick",
    "test:junit": "./scripts/test.sh --junit",
    "test:standalone": "./scripts/test-standalone.sh",
    "test:cluster": "./scripts/test-cluster.sh",
    "test:json": "./scripts/test-json.sh",
    "valkey:start": "./scripts/valkey.sh start",
    "valkey:stop": "./scripts/valkey.sh stop",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "dev": "concurrently \"npm run build:watch\" \"npm run test:quick\"",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "keywords": [
    "GLIDE",
    "redis",
    "valkey",
    "ioredis",
    "ioredis-replacement",
    "valkey-glide",
    "bull",
    "bullmq",
    "job-queue",
    "socketio",
    "express-session",
    "redis-adapter",
    "high-performance",
    "rust",
    "typescript",
    "drop-in-replacement",
    "redis-client",
    "cache",
    "database",
    "pub-sub",
    "streams",
    "cluster",
    "redis-compatible",
    "nodejs",
    "memory-store",
    "session-store",
    "rate-limiting",
    "real-time",
    "microservices",
    "scaling",
    "performance",
    "json",
    "valkeyjson",
    "redisjson",
    "zero-code-changes",
    "100-percent-compatible"
  ],
  "author": {
    "name": "Avi Fenesh",
    "email": "avifenesh@gmail.com",
    "url": "https://github.com/avifenesh"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avifenesh/valkey-glide-ioredis-adapter.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/avifenesh/valkey-glide-ioredis-adapter/issues"
  },
  "homepage": "https://github.com/avifenesh/valkey-glide-ioredis-adapter",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@valkey/valkey-glide": "^2.1.0",
    "events": "^3.3.0",
    "msgpackr": "^1.11.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/chai": "^5.2.2",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "bee-queue": "^1.7.1",
    "bull": "^4.16.5",
    "bullmq": "^5.58.7",
    "chai": "^6.0.1",
    "concurrently": "9.2.1",
    "connect-redis": "^9.0.0",
    "dotenv": "17.2.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "5.5.4",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "prettier": "3.6.2",
    "rate-limit-redis": "^4.2.2",
    "rimraf": "6.0.1",
    "semantic-release": "^24.2.9",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "why-is-node-running": "^3.2.2"
  },
  "peerDependencies": {
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  }
}
